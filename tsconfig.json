{
	"compilerOptions": {
	  /* Visit https://aka.ms/tsconfig.json to read more about this file */
  
	  "target": "esnext",
	  "module": "commonjs",
	  "moduleResolution": "node",
	  "baseUrl": ".",
	  "rootDir": "src",
	  "outDir": "dist",
	  "esModuleInterop": true,
	  "forceConsistentCasingInFileNames": true,
  
	  "strict": true,
	  "skipLibCheck": true
	},
	"include": ["src/**/*.ts"],
	"exclude": ["src/**/__tests__"]
}
